{
    "name": "mikefrancis/laravel-secureheaders",
    "description": "SecureHeaders wrapper for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Francis",
            "email": "mikeffrancis@gmail.com"
        }
